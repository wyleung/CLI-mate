  <rdf:Description rdf:nodeID="ugZTdlRX8">
    <process:name>ls</process:name>
    <dcterms:description>List files in a directory.</dcterms:description>
    <rdf:type rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl#Process"/>
    <dcterms:creator rdf:nodeID="ugZTdlRX9"/>
